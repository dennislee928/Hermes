# Hermes - Cybersecurity Provider Integration Service
# Copy to .env and fill in values. Do not commit .env.

# Server
HTTP_PORT=8080
LOG_LEVEL=info

# Database (used when running with Docker or local Postgres)
POSTGRES_DSN=host=postgres user=hermes password=changeme dbname=hermes sslmode=disable
# Local: POSTGRES_DSN=host=localhost user=hermes password=changeme dbname=hermes sslmode=disable

# Cache
CACHE_TTL_SECONDS=3600

# Provider API keys (leave empty to skip provider)
ABUSEIPDB_API_KEY=
VIRUSTOTAL_API_KEY=
PHISHTANK_APP_KEY=
GOOGLE_SAFE_BROWSING_API_KEY=
URLSCAN_API_KEY=
HIBP_API_KEY=
NVD_API_KEY=
BINARYEDGE_API_KEY=
CRIMINALIP_API_KEY=
PULSEDIVE_API_KEY=
EMAILREP_API_KEY=
VULNERS_API_KEY=
HYBRIDANALYSIS_API_KEY=
MALSHARE_API_KEY=
MALWAREBAZAAR_API_KEY=
